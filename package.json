    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^6.0.3",
    "focus-visible": "^5.2.0",
    "inputmask": "^5.0.7",
    "jquery": "^3.6.0",
    "laravel-mix": "^6.0.0",
    "swiper": "^8.4.4",
    "vanilla-lazyload": "^17.5.0",
    "bootstrap": "^4.6.2",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "vue-template-compiler": "^2.7.14"
  }
