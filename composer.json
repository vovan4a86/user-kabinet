{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=7.2.5",
		"barryvdh/laravel-debugbar": "~3.2",
		"barryvdh/laravel-ide-helper": "~2.6",
		"doctrine/dbal": "~2.9",
		"fideloper/proxy": "~4.0",
		"guzzlehttp/guzzle": "~7.2",
		"htmlmin/htmlmin": "~7.0",
		"intervention/image": "~2.4",
		"laravel/framework": "^7.0",
		"laravel/tinker": "~2.0",
		"laravelcollective/html": "~6.0",
		"phpspec/phpspec": "~6.3",
		"lavary/laravel-menu": "^1.7",
		"artesaos/seotools": "~0.18",
		"laravel/helpers": "^1.2"
	},
	"require-dev": {
		"filp/whoops": "~2.0",
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "~1.0",
		"nunomaduro/collision": "~4.1",
		"phpunit/phpunit": "~8.5",
		"symfony/thanks": "^1.0",
		"symfony/css-selector": "3.1.*",
		"symfony/dom-crawler": "^5.4",
		"beyondcode/laravel-query-detector": "^1.8"
	},
	"autoload": {
        "files": [
          "app/helpers.php"
        ],
		"classmap": [
			"database/seeds",
			"database/factories",
			"packages/unisharp/laravel-filemanager/src"
		],
		"psr-4": {
			"App\\": "app/",
			"Fanky\\Admin\\": "packages/fanky/admin/src/",
			"Fanky\\Auth\\": "packages/fanky/auth/src/",
			"Unisharp\\Laravelfilemanager\\": "packages/unisharp/laravel-filemanager/src"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
			"@php artisan key:generate"
		],
		"post-create-project-cmd": [
			"@php artisan key:generate"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover"
		]
	},
	"config": {
		"preferred-install": "dist",
		"platform": {
			"php": "7.2.5"
		},
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
